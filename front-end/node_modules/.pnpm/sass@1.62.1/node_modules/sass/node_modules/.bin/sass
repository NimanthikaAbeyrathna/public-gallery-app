#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/sass@1.62.1/node_modules/sass/node_modules:/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/sass@1.62.1/node_modules:/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/sass@1.62.1/node_modules/sass/node_modules:/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/sass@1.62.1/node_modules:/home/nimanthika/Documents/dep-10/phase 2/public-galery-app/front-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../sass.js" "$@"
else
  exec node  "$basedir/../../sass.js" "$@"
fi
